{
  "class": [
    [
      {
        "type": "三角形",
        "default": {
          "border-color":"transparent transparent #000 transparent"
        },
        "property": [ "border-width" ],
        "regular": {
          "border-width": "^([0-9]+px) ([0-9]+px) ([0-9]+px) ([0-9]+px)$"
        }
      },
      {
        "type": "四角形",
        "default": {
          "border-color":"#000",
          "background":"#000"
        },
        "property": [ "width", "height" ],
        "regular": {
          "width": "^([0-9]+px)$",
          "height": "^([0-9]+px)$"
        }
      },
      {
        "type": "円",
        "default": {
          "border-color":"#000",
          "background":"#000"
        },
        "property": [ "width", "height", "border-radius" ],
        "regular": {
          "width": "^([0-9]+px)$",
          "height": "^([0-9]+px)$",
          "border-radius": "^([1-9]|[1-9][0-9]|100)%$"
        }
      }
    ],
    [
      {
        
      }
    ]
  ]
}